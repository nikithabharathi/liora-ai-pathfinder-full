{
  "name": "fullstack-llm-project",
  "version": "1.0.0",
  "description": "Fullstack project with frontend (Vite) and backend (Express) for LLM integration",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev-frontend": "vite",
    "dev-backend": "nodemon server.js",
